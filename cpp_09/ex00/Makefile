SRC = main.cpp\
BitcoinExchange.cpp\

NAME = btc

$(NAME)	:	$(SRC)
	c++ -Wall -Werror -Wextra -std=c++98 $(SRC) -o $(NAME) -Ofast

all		:	$(NAME)

clean	:
	rm -f $(NAME)

fclean	:
	rm -f $(NAME)

re		:	fclean all

test	: $(NAME)
	@ printf "date|value\n2023-01-31|1\n2023-02-31|1\n2023-03-31|1\n2023-04-31|1\n2023-05-31|1\n2023-06-31|1\n2023-07-31|1\n2023-08-31|1\n2023-09-31|1\n2023-10-31|1\n2023-11-31|1\n2023-12-31|1\n2023-02-29|1\n2020-02-29|1\n2000-02-29|1\n1200-02-29|1\n1000-02-29|1\n0400-02-29|1\n2023-04-14|1\n2023-00-01|1\n2023-13-01|1\n2023-04-00|1\n2023-04-32|1\n2023-04-14|1\n22023-04-14|1\n202-04-14|1\n2023-4-14|1\n2023-004-14|1\n2023-04-014|1\n2023-04-4|1\n2023-0414|1\n2023--04--14v|1\n2023/04/14|1\n2023 - 04 - 14|1\n202~-04-14|1\n2023-0~-14|1\n2023-04-1~|1\n2023-04-14|1\n2001-04-14|1\n2023-05-04|1\n2023-02-28|1\n2020-02-29|1\n1999-12-31|1\n2000-01-01|1\n1969-12-31|1\n1970-01-01|1\n9999-12-31|1\n2147483648-2147483648-2147483648|1\n2020-10-11|1001\n2020-10-11|9999999999999999999999999999999999999999999999999999.999\n2020-10-11|-80\n2020-10-11|123a123\n2020-10-11 1\n" > input.txt
	./btc input.txt
.PHONY	: all clean fclean re test